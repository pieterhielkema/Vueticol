{
  "name": "vueticol",
  "version": "1.0.2",
  "description": "Pretty color picker for Vue ðŸŒˆ",

  "main": "dist/vueticol.ssr.js",
  "module": "dist/vueticol.esm.js",
  "unpkg": "dist/vueticol.min.js",
  "browser": {
    "./sfc": "src/vueticol.vue"
  },

  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vueticol.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vueticol.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vueticol.min.js"
  },
  "devDependencies": {
    "rollup": "^3.19.0",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.47",
    "vue-template-compiler": "^2.7.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pieterhielkema/Vueticol.git"
  },
  "author": "Pieter Hielkema",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pieterhielkema/Vueticol/issues"
  },
  "homepage": "https://github.com/pieterhielkema/Vueticol#readme"
}
